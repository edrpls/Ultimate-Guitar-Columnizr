{
  "manifest_version": 2,
  "name": "Ultimate Guitar Columnizr",
  "description": "Unofficial extension for turning ultimate-guitar.com into a multicolumn page.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
